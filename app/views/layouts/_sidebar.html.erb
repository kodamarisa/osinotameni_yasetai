<div class="sidebar">
  <ul>
    <% if session[:current_calendar_id].present? %>
      <li><%= link_to 'Calendar', calendar_path(session[:current_calendar_id]) %></li>
    <% end %>
    <% if user_signed_in? %>
      <%= link_to 'My Profile', user_profile_path %>
    <% else %>
      <%= link_to 'Register', new_registration_path %>
    <% end %>
    <li><a href="/exercises">Exercises</a></li>
  </ul>
</div>
