<%= form_with(model: @calendar, local: true) do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div>
      <%= form.label :color %>
      <%= form.text_field :color %>
    </div>
    <div>
      <%= form.label :image_url %>
      <%= form.text_field :image_url %>
    </div>
    <div>
      <%= form.submit %>
    </div>
  <% end %>